;
; BIND data file for ARdomain.net
;
$ORIGIN ARdomain.net.
$TTL 1D
@	IN	SOA	ARdomain.net. root.ARdomain.net. (
			1		; Serial
			604800		; Refresh
			86400		; Retry
			2419200		; Expire
			604800 )	; Default TTL
;
		IN	NS	ubuntu.ARdomain.net.
;
dns		IN	CNAME	ubuntu
;
ubuntu	IN	A		127.0.0.1
PC-Debian-12	IN	A		127.0.0.1
